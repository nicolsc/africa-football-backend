<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Users</title>
<!--<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>-->
    <script type="text/javascript" src="/js/jquery.js"></script>    <link rel="stylesheet" href="/css/style.css" />
  </head>
  <body>
    <h1>Users</h1>
     <% if (users && users.length){%>
    <table id="users">
      <thead>
        <tr>
          <th>#</th>
          <th>Name</th>
          <th>Admin</th>
          <th>Registered date</th>
          <th>IP Address</th>
        </tr>
      </thead>
      <tbody>
      <% users.forEach(function(user, idx){%>
        <tr class='user'>
          <td><%=idx+1%>.</td>
          <td><%=user.name%></td>
          <td><%=user.admin ? 'true' : 'false'%></td>
          <td><%=user.createdDate ? user.createdDate.toLocaleString() : ''%></td>
          <td><%=user.registrationIP || ''%></td>
        </tr>

    <%}); %>
       
      </tbody>
    </table>
    <% }
  else{%>
    <h3>Nothing yet</h3>
  <%}%>
  </body>
</html>