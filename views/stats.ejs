<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Support stats</title>
<!--<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>-->
    <script type="text/javascript" src="/js/jquery.js"></script>    <link rel="stylesheet" href="/css/style.css" />
  </head>
  <body>
    <h1>Connection stats</h1>
    <% if (stats){%>
      <% if (stats.paths && Object.keys && Object.keys(stats.paths).length){%>
        <table id="stats-paths">
          <thead>
            <tr>
              <th>Path</th>
              <th>Hits</th>
            </tr>
          </thead>
          <tbody>
          <% stats.paths.forEach(function(item){%>
            <tr class='stat-path'>
              <td><%=item.path%></td>
              <td><%=item.count%></td>
            </tr>

          <%}); %>
          </tbody>
        </table>
      <% } %>
    <% }
    else{%>
      <h3>Nothing yet</h3>
    <%}%>
  </body>
</html>